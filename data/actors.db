ue":[3,5,7,9,11,13,15,17,19]},"abilityBoostLevels":{"value":[5,10,15,20]}},"effects":[{"_id":"snhfsEvZXQcSxaTF","changes":[{"key":"data.attributes.perception.rank","mode":4,"value":"1"},{"key":"data.saves.fortitude.rank","mode":4,"value":"1"},{"key":"data.saves.reflex.rank","mode":4,"value":"1"},{"key":"data.saves.will.rank","mode":4,"value":"2"},{"key":"data.skills.arc.rank","mode":4,"value":"1"},{"key":"data.martial.weapon-base-club.rank","mode":4,"value":"1"},{"key":"data.martial.weapon-base-crossbow.rank","mode":4,"value":"1"},{"key":"data.martial.weapon-base-dagger.rank","mode":4,"value":"1"},{"key":"data.martial.weapon-base-heavy-crossbow.rank","mode":4,"value":"1"},{"key":"data.martial.weapon-base-staff.rank","mode":4,"value":"1"},{"key":"data.martial.unarmed.rank","mode":4,"value":"1"},{"key":"data.martial.unarmored.rank","mode":4,"value":"1"}],"disabled":false,"duration":{"startTime":null},"flags":{},"icon":"systems/pf2e/icons/classes/wizard.webp","label":"Initial Proficiencies","tint":null,"transfer":true},{"_id":"85WGHfJbFcoWzuci","changes":[{"key":"data.details.keyability.value","mode":5,"value":"int"}],"disabled":false,"duration":{"startTime":null},"flags":{},"icon":"systems/pf2e/icons/classes/wizard.webp","label":"Key Ability","tint":null,"transfer":true}],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.classes.RwjIZzIxzPpUglnK"},"pf2e":{"insertedClassFeaturesLevel":1}}},{"_id":"hmcohg1kt8f4m4rq","name":"Pilgrim","type":"background","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/default-icons/mystery-man.svg","data":{"description":{"value":"<p>In a dusty shop, market stall, or merchant caravan, you bartered wares for coin and trade goods. The skills you picked up still apply in the adventuring life, in which a good deal on a suit of armor could prevent your death.</p>\n<p>Choose two ability boosts. One must be to <strong>Intelligence</strong> or <strong>Charisma</strong>, and one is a free ability boost.</p>\n<p>You're trained in the Diplomacy skill and the Mercantile Lore skill. You gain the @Compendium[pf2e.feats-srd.5nc5ridFBfYpn2Om]{Bargain Hunter} skill feat.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":[],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"merchant","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:23.075-05:00","version":{"schema":null}}},"boosts":{"0":{"value":["cha","int"]},"1":{"value":["cha","con","dex","int","str","wis"]}},"items":{},"trainedLore":"Nethys Lore","trainedSkills":{"value":["rel"],"custom":""},"usage":{"value":"held-in-one-hand"}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.backgrounds.KMv7ollLVaZ81XDV"}}},{"_id":"dhUWmS9rkRufbc9w","name":"Versatile Heritage (Canny Acumen)","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/ancestry/versatile-heritage.webp","data":{"description":{"value":"<p>Humanity's versatility and ambition have fueled its ascendance to be the most common ancestry in most nations throughout the world. Select a general feat of your choice for which you meet the prerequisites (as with your ancestry feat, you can select this general feat at any point during character creation).</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["human"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"versatile-heritage","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:22.931-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"heritage"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[]},"location":""},"effects":[],"folder":null,"sort":600000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.ancestryfeatures.hFBwsVcSnNCJoimo"}}},{"_id":"2HVEHbeuj6vmgd3w","name":"Canny Acumen (Perception)","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/feats/feats.webp","data":{"description":{"value":"<p>Your avoidance or observation is beyond the ken of most in your profession. Choose Fortitude saves, Reflex saves, Will saves, or Perception. You become an expert in your choice. At 17th level, you become a master in your choice.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["general"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"canny-acumen","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:28.649-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"general"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[]},"location":""},"effects":[],"folder":null,"sort":700000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.feats-srd.i4yRvVwvXbGZDsD1"}}},{"_id":"H2KUz18iqfXoQhzK","name":"Natural Ambition (Reach Spell)","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/feats/feats.webp","data":{"description":{"value":"<p>You were raised to be ambitious and always reach for the stars, leading you to progress quickly in your chosen field. You gain a 1st-level class feat for your class. You must meet the prerequisites, but you can select the feat later in the character creation process in order to determine which prerequisites you meet.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["human"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"natural-ambition","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:29.195-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"ancestry"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[]},"location":"ancestry-1"},"effects":[],"folder":null,"sort":800000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.feats-srd.PodajLVxqYSAqVox"}}},{"_id":"o7iRIooqEZoEK3vz","name":"Reach Spell","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/feats/feats.webp","data":{"description":{"value":"<p>You can extend the range of your spells. If the next action you use is to @Compendium[pf2e.actionspf2e.aBQ8ajvEBByv45yz]{Cast a Spell} that has a range, increase that spell's range by 30 feet. As is standard for increasing spell ranges, if the spell normally has a range of touch, you extend its range to 30 feet.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["bard","cleric","concentrate","druid","metamagic","oracle","sorcerer","witch","wizard"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"reach-spell","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:29.322-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"class"},"actionType":{"value":"action"},"actionCategory":{"value":""},"actions":{"value":"1"},"prerequisites":{"value":[]},"location":""},"effects":[],"folder":null,"sort":900000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.feats-srd.BWomK7EVY0WXxWgh"}}},{"_id":"mLSPoFIlyJ51CBeX","name":"Nethys Lore","type":"lore","img":"systems/pf2e/icons/default-icons/lore.svg","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":null,"schema":{"version":0.642,"lastMigration":null},"mod":{"value":0},"proficient":{"value":1}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{}},{"_id":"GExkNrUrfCjbBLyS","name":"Prepared Arcane Spells","type":"spellcastingEntry","img":"systems/pf2e/icons/default-icons/spellcastingEntry.svg","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":null,"schema":{"version":0.642,"lastMigration":null},"ability":{"value":"int"},"spelldc":{"value":7,"dc":17,"mod":4},"tradition":{"value":"arcane"},"focus":{"points":1,"pool":1},"prepared":{"value":"prepared"},"showUnpreparedSpells":{"value":true},"proficiency":{"value":1},"displayLevels":{},"slots":{"slot0":{"prepared":{"0":{"id":"rV3FmZreN473ykP0"},"1":{"id":"gqIUAuAVHnuhRku2"},"2":{"id":"gskdXpatYdpayubU"},"3":{"id":"pcnh0bqo1mn18E7U"},"4":{"id":"Q9UB61fNJ0ZL8lav"},"5":{"id":"LQLZdoRyHI5qbloe"},"6":{"id":"ou9txDeGtlZ0ELKk"}},"value":0,"max":7},"slot1":{"prepared":{"0":{"id":"AuZWKdIRU539GaIe","expended":false},"1":{"id":"y7YW6GBy5w0LvQNj","expended":true},"2":{"id":"Lbiro6isx4WktlxW"}},"value":0,"max":3},"slot2":{"prepared":[],"value":0,"max":0},"slot3":{"prepared":[],"value":0,"max":0},"slot4":{"prepared":[],"value":0,"max":0},"slot5":{"prepared":[],"value":0,"max":0},"slot6":{"prepared":[],"value":0,"max":0},"slot7":{"prepared":[],"value":0,"max":0},"slot8":{"prepared":[],"value":0,"max":0},"slot9":{"prepared":[],"value":0,"max":0},"slot10":{"prepared":[],"value":0,"max":0},"slot11":{"prepared":[],"value":0,"max":0}},"signatureSpells":{"value":[]},"autoHeightenLevel":{"value":null}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{}},{"_id":"QSywvaESq1sA4pGD","name":"Pilgrim's Token","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/feats/feats.webp","data":{"description":{"value":"<p>You carry a small token of protection from a site holy to your faith, or you touched your religious symbol to a relic or altar at such a site. So long as this token is in your possession, when you tie an adversary's initiative roll, you go first.</p>\n<p><strong>Special</strong> If you select this feat at 1st level, you receive your pilgrim's token for free. Alternately, if you have a religious symbol, it is already attuned, as described above.</p>\n<p>If you select this feat at a later level, or if you lose your pilgrim's token, you must purchase or Craft a replacement and attune it at a holy site. Such a token usually costs at least 2 sp, and the attunement takes 10 minutes of prayer and requires a successful DC 20 Religion check. Your GM might adjust the price and DC depending on the token's material and quality and the religious significance of the site; the more significant the location, the easier the attunement.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["general","skill"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"pilgrims-token","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:29.261-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"skill"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[{"value":"trained in Religion"}]},"location":""},"effects":[],"folder":null,"sort":1100000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.feats-srd.BqceQIKE0lwIS98s"}}},{"_id":"yhLrJpsExk5mYhmD","name":"Arcane Thesis: Staff Nexus","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/classes/staff-nexus.webp","data":{"description":{"value":"<p>Your thesis maintains that early and intense adoption of staves from the first days of study can create a symbiotic bond between spellcaster and @Compendium[pf2e.equipment-srd.FVjTuBCIefAgloUU]{Staff}, allowing them to create remarkable magic together. You've formed such a connection with a makeshift staff you built, and you are ready to infuse any staff you encounter with greater power.</p>\n<p>You begin play with a makeshift staff of your own invention. It contains one cantrip and one 1st-level spell, both from your spellbook, but it gains no charges normally during your preparations; you must expend a spell slot to grant it charges in the same way you would add additional charges to a normal staff. You can @Compendium[pf2e.actionspf2e.rmwa3OyhTZ2i2AHl]{Craft} your makeshift staff into any other type of staff for the new staff's usual cost, adding the two spells you originally chose to the staff you Craft.</p>\n<p>At 8th level, you can expend two spells instead of one when preparing your staff, adding additional charges equal to the combined levels of the expended spells. At 16th level, you can expend up to a total of three spells to add charges to the staff, adding additional charges equal to the combined levels of all three spells.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["wizard"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"arcane-thesis-staff-nexus","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:23.654-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"classfeature"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[]},"location":""},"effects":[],"folder":null,"sort":1000000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.classfeatures.Klb35AwlkNrq1gpB"}}},{"_id":"sh9gb9WHMfdRMoB4","name":"Arcane School: Conjuration","type":"feat","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/features/classes/conjuration.webp","data":{"description":{"value":"<p>As a conjurer, you summon creatures and objects from places beyond, and use magic to transport to distant locales. You understand that the true key to victory is strength in numbers. You add one 1st-level conjuration spell (such as @Compendium[pf2e.spells-srd.4YnON9JHYqtLzccu]{Summon Animal}) to your spellbook. You learn the @Compendium[pf2e.spells-srd.tWzxuJdbXqvskdIo]{Augment Summoning} school spell.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["wizard"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"arcane-school-conjuration","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:03:23.648-05:00","version":{"schema":null}}},"level":{"value":1},"featType":{"value":"classfeature"},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":""},"prerequisites":{"value":[]},"location":""},"effects":[],"folder":null,"sort":200000,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.classfeatures.SNeVaUBTHwvoO6kr"}}},{"_id":"AuZWKdIRU539GaIe","name":"Summon Animal","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/summon-animal.webp","data":{"description":{"value":"<p>You conjure an animal to fight for you. You summon a common creature that has the animal trait and whose level is -1, such as those found in Pathfinder Bestiary volumes. Heightening the spell increases the maximum level of creature you can summon.</p>\n<hr />\n<p><strong>Heightened (2nd)</strong> Level 1</p>\n<p><strong>Heightened (3rd)</strong> Level 2</p>\n<p><strong>Heightened (4th)</strong> Level 3</p>\n<p><strong>Heightened (5th)</strong> Level 5</p>\n<p><strong>Heightened (6th)</strong> Level 7</p>\n<p><strong>Heightened (7th)</strong> Level 9</p>\n<p><strong>Heightened (8th)</strong> Level 11</p>\n<p><strong>Heightened (9th)</strong> Level 13</p>\n<p><strong>Heightened (10th)</strong> Level 15</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["conjuration"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"summon-animal","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.558-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"]},"school":{"value":"conjuration"},"components":{"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"3"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.4YnON9JHYqtLzccu"}}},{"_id":"DV3XeLMhx936Y4Js","name":"Grease","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/grease.webp","data":{"description":{"value":"<p>You conjure grease, with effects based on choosing area or target.</p>\n<ul>\n<li><strong>Area</strong> <strong>[4 contiguous 5-foot squares]</strong> All solid ground in the area is covered with grease. Each creature standing on the greasy surface must succeed at a Reflex save or an Acrobatics check against your spell DC or fall @Compendium[pf2e.conditionitems.j91X7x0XSomq8d60]{Prone}. Creatures using an action to move onto the greasy surface during the spell's duration must attempt either a Reflex save or an Acrobatics check to Balance. A creature that Steps or Crawls doesn't have to attempt a check or save.</li>\n<li><strong>Target</strong> <strong>[1 object of Bulk 1 or less]</strong> If you cast the spell on an unattended object, anyone trying to pick up the object must succeed at an Acrobatics check or Reflex save against your spell DC to do so. If you target an attended object, the creature that has the object must attempt an Acrobatics check or Reflex save. On a failure, the holder or wielder takes a -2 circumstance penalty to all checks that involve using the object; on a critical failure, the holder or wielder releases the item. The object lands in an adjacent square of the GM's choice. If you cast this spell on a worn object, the wearer gains a +2 circumstance bonus to Fortitude saves against attempts to grapple them.</li>\n</ul>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["conjuration"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"grease","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.236-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"]},"school":{"value":"conjuration"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object of Bulk 1 or less"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"reflex","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"4 contiguous 5-foot squares or Targets 1 object of 1 Bulk or less"},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Wu0xFpewMKRK3HG8"}}},{"_id":"Lbiro6isx4WktlxW","name":"Color Spray","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/color-spray.webp","data":{"description":{"value":"<p>Swirling colors affect viewers based on their Will saves.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature is @Compendium[pf2e.conditionitems.TkIyaNPgTZFBCCuh]{Dazzled} for 1 round.</p>\n<p><strong>Failure</strong> The creature is @Compendium[pf2e.conditionitems.dfCMdR4wnpbYNTix]{Stunned 1}, @Compendium[pf2e.conditionitems.XgEqL1kFApUbl5Z2]{Blinded} for 1 round, and Dazzled for 1 minute.</p>\n<p><strong>Critical Failure</strong> The creature is Stunned for 1 round and Blinded for 1 minute.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["illusion","incapacitation","visual"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"color-spray","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.064-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"illusion"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":15,"areaType":"cone"},"time":{"value":"2"},"duration":{"value":"1 or more rounds (see below)"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":"15-foot cone"},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.UKsIOWmMx4hSpafl"}}},{"_id":"y7YW6GBy5w0LvQNj","name":"Mage Armor","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/mage-armor.webp","data":{"description":{"value":"<p>You ward yourself with shimmering magical energy, gaining a +1 item bonus to AC and a maximum Dexterity modifier of +5. While wearing mage armor, you use your unarmored proficiency to calculate your AC.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> You gain a +1 item bonus to saving throws.</p>\n<p><strong>Heightened (6th)</strong> The item bonus to AC increases to +2, and you gain a +1 item bonus to saving throws.</p>\n<p><strong>Heightened (8th)</strong> The item bonus to AC increases to +2, and you gain a +2 item bonus to saving throws.</p>\n<p><strong>Heightened (10th)</strong> The item bonus to AC increases to +3, and you gain a +3 item bonus to saving throws.</p>\n\n<p>@Compendium[pf2e.spell-effects.qkwb5DD3zmKwvbk0]{Spell Effect: Mage Armor}</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["abjuration"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"mage-armor","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.322-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"abjuration"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"until the next time you make your daily preparations"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.aAbfKn8maGjJjk2W"}}},{"_id":"rV3FmZreN473ykP0","name":"Light","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/light.webp","data":{"description":{"value":"<p>The object glows, casting bright light in a 20-foot radius (and dim light for the next 20 feet) like a torch. If you cast this spell again on a second object, the light spell on the first object ends.</p>\n<hr />\n<p><strong>Heightened (4th)</strong> The object sheds bright light in a 60-foot radius (and dim light for the next 60 feet).</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["light","cantrip","evocation"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"light","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.310-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"evocation"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object of 1 Bulk or less, either unattended or possessed by you or a willing ally"},"range":{"value":"touch"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"until the next time you make your daily preparations"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.WBmvzNDfpwka3qT4"},"enhanced-terrain-layer":{"multiple":1,"min":0,"max":0,"environment":""}}},{"_id":"gqIUAuAVHnuhRku2","name":"Tanglefoot","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/tanglefoot.webp","data":{"description":{"value":"<p>A vine covered in sticky sap appears from thin air, flicking from your hand and lashing itself to the target. Attempt a spell attack against the target.</p>\n<hr />\n<p><strong>Critical Success</strong> The target gains the @Compendium[pf2e.conditionitems.eIcWbB5o3pP6OIMe]{Immobilized} condition and takes a -10-foot circumstance penalty to its Speeds for 1 round. It can attempt to Escape against your spell DC to remove the penalty and the @Compendium[pf2e.conditionitems.eIcWbB5o3pP6OIMe]{Immobilized} condition.</p>\n<p><strong>Success</strong> The target takes a -10-foot circumstance penalty to its Speeds for 1 round. It can attempt to Escape against your spell DC to remove the penalty.</p>\n<p>@Compendium[pf2e.spell-effects.TwtUIEyenrtAbeiX]{Spell Effect: Tanglefoot}</p>\n<p><strong>Failure</strong> The target is unaffected.</p>\n<hr />\n<p><strong>Heightened (2nd)</strong> The effects last for 2 rounds.</p>\n<p><strong>Heightened (4th)</strong> The effects last for 1 minute.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["attack","cantrip","conjuration","plant"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"tanglefoot","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.577-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"attack"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"],"custom":""},"school":{"value":"conjuration"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.uZK2BYzPnxUBnDjr"}}},{"_id":"gskdXpatYdpayubU","name":"Mage Hand","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/mage-hand.webp","data":{"description":{"value":"<p>You create a single magical hand, either invisible or ghostlike, that grasps the target object and moves it slowly up to 20 feet. Because you're levitating the object, you can move it in any direction. When you Sustain the Spell, you can move the object an additional 20 feet. If the object is in the air when the spell ends, the object falls.</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> You can target an unattended object with a Bulk of 1 or less.</p>\n<p><strong>Heightened (5th)</strong> The range increases to 60 feet, and you can target an unattended object with a Bulk of 1 or less.</p>\n<p><strong>Heightened (7th)</strong> The range increases to 60 feet, and you can target an unattended object with a Bulk of 2 or less.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["cantrip","evocation"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"mage-hand","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.323-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult"]},"school":{"value":"evocation"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 unattended object of light Bulk or less"},"range":{"value":"30 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.pwzdSlJgYqN7bs2w"}}},{"_id":"pcnh0bqo1mn18E7U","name":"Prestidigitation","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/prestidigitation.webp","data":{"description":{"value":"<p>The simplest magic does your bidding. You can perform simple magical effects for as long as you Sustain the Spell. Each time you Sustain the Spell, you can choose one of four options.</p>\n<ul>\n<li><strong>Cook</strong> Cool, warm, or flavor 1 pound of nonliving material.</li>\n<li><strong>Lift</strong> <strong>Slowly</strong> lift an unattended object of light Bulk or less 1 foot off the ground.</li>\n<li><strong>Make</strong> Create a temporary object of negligible Bulk, made of congealed magical substance. The object looks crude and artificial and is extremely fragile-it can't be used as a tool, weapon, or spell component.</li>\n<li><strong>Tidy</strong> Color, clean, or soil an object of light Bulk or less. You can affect an object of 1 Bulk with 10 rounds of concentration, and a larger object a 1 minute per Bulk.</li>\n</ul>\n<p>Prestidigitation can't deal damage or cause adverse conditions. Any actual change to an object (beyond what is noted above) persists only as long as you Sustain the Spell.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["cantrip","evocation"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"prestidigitation","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.409-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"evocation"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 object (cook, lift, or tidy only)"},"range":{"value":"10 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":"sustained"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":true},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.Qw3fnUlaUbnn7ipC"}}},{"_id":"Q9UB61fNJ0ZL8lav","name":"Shield","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/shield.webp","data":{"description":{"value":"<p>You raise a magical shield of force. This counts as using the Raise a Shield action, giving you a +1 circumstance bonus to AC until the start of your next turn, but it doesn't require a hand to use.</p>\n<p>While the spell is in effect, you can use the Shield Block reaction with your magic shield. The shield has Hardness 5. After you use Shield Block, the spell ends and you can't cast it again for 10 minutes. Unlike a normal Shield Block, you can use the spell's reaction against the magic missile spell.</p>\n<p>Heightening the spell increases the shield's Hardness.</p>\n<p>@Compendium[pf2e.spell-effects.Jemq5UknGdMO7b73]{Spell Effect: Shield}</p>\n<hr />\n<p><strong>Heightened (3rd)</strong> The shield has Hardness 10.</p>\n<p><strong>Heightened (5th)</strong> The shield has Hardness 15.</p>\n<p><strong>Heightened (7th)</strong> The shield has Hardness 20.</p>\n<p><strong>Heightened (9th)</strong> The shield has Hardness 25.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["abjuration","cantrip","force"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"shield","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.497-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"abjuration"},"components":{"material":false,"somatic":false,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":null,"areaType":""},"time":{"value":"1"},"duration":{"value":"until the start of your next turn"},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.TVKNbcgTee19PXZR"}}},{"_id":"LQLZdoRyHI5qbloe","name":"Ray of Frost","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/ray-of-frost.webp","data":{"description":{"value":"<p>You blast an icy ray. Make a spell attack roll. The ray deals cold damage equal to 1d4 + your spellcasting ability modifier.</p>\n<hr />\n<p><strong>Critical Success</strong> The target takes double damage and takes a -10-foot status penalty to its Speeds for 1 round.</p>\n<p>@Compendium[pf2e.spell-effects.I4PsUAaYSUJ8pwKC]{Spell Effect: Ray of Frost}</p>\n<p><strong>Success</strong> The target takes normal damage.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d4.</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["attack","cold","cantrip","evocation"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"ray-of-frost","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.435-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"attack"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"],"custom":""},"school":{"value":"evocation"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":"1d4","applyMod":true},"damageType":{"value":"cold"},"scaling":{"mode":"level1","formula":"1d4"},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""},"location":{"value":"GExkNrUrfCjbBLyS"},"hasCounteractCheck":{"value":false},"autoHeightenLevel":{"value":null},"areasize":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"bL4m8T94kYDMr79h":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.gYjPm7YwGtEa1oxh"}}},{"_id":"ou9txDeGtlZ0ELKk","name":"Detect Magic","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/detect-magic.webp","data":{"description":{"value":"<p>You send out a pulse that registers the presence of magic. You receive no information beyond the presence or absence of magic. You can choose to ignore magic you're fully aware of, such as the magic items and ongoing spells of you and your allies.</p>\n<p>You detect illusion magic only if that magic's effect has a lower level than the level of your detect magic spell. However, items that have an illusion aura but aren't deceptive in appearance (such as an invisibility potion) typically are detected normally.</p>\n<p><strong>Heightened (3rd)</strong> You learn the school of magic for the highest-level effect within range that the spell detects. If multiple effects are equally strong, the GM determines which you learn.</p>\n<p><strong>Heightened (4th)</strong> As 3rd level, but you also pinpoint the source of the highest-level magic. Like for an imprecise sense, you don't learn the exact location, but can narrow down the source to within a 5-foot cube (or the nearest if larger than that).</p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["detection","cantrip","divination"],"rarity":{"value":"common"},"custom":""},"rules":[],"slug":"detect-magic","schema":{"version":0.642,"lastMigration":{"datetime":"2021-07-03T22:04:00.106-05:00","version":{"schema":null}}},"level":{"value":1},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult","primal"],"custom":""},"school":{"value":"divination"},"components":{"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"30","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":"","applyMod":false},"damageType":{"value":""},"scaling":{"mode":"none","formula":""},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"prepared":{"value":""}